{
  "name": "rocareer/radmin",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "https://www.rocareer.com",
  "license": "Apache-2.0",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "albert",
      "email": "albert@rocareer.com",
      "homepage": "https://www.rocareer.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "albert@rocareer.com",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "https://wenda.workerman.net/",
    "wiki": "https://workerman.net/doc/webman",
    "source": "https://github.com/walkor/webman"
  },
  "require": {
    "php": ">=8.1",

    "workerman/webman-framework": "^2.1",
    "webman/event": "^1.0",
    "webman/cache": "^2.1",
    "webman/console": "^2.1",
    "monolog/monolog": "^2.0",


    "rocareer/webman-migration": "dev-master",
    "rocareer/webman-status-code": "dev-master",
    "firebase/php-jwt": "^6.11",
    "webman/think-orm": "^2.1",
    "vlucas/phpdotenv": "^5.6",
    "nelexa/zip": "^4.0",
    "ext-bcmath": "*",
    "ext-iconv": "*",
    "ext-pdo": "*",
    "ext-gd": "*",
    "phpmailer/phpmailer": "^v6.8.1"

  },
  "require-dev": {
    "rocareer/webman-dev": "dev-master",
    "phpunit/phpunit": "^9.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://gitee.com/rocareer/radmin.git"
    }
  ],
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app",
      "App\\": "./app",
      "app\\View\\": "./app/view/"
    }
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
